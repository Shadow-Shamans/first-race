{
  "name": "client",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "npm": ">=8 <=9",
    "node": ">=16 <=18"
  },
  "scripts": {
    "dev": "vite",
    "build:client": "vite build --config vite.config.ts",
    "build:ssr": "vite build --config ssr.config.ts",
    "prebuild:client": "rimraf --glob 'dist'",
    "prebuild:ssr": "rimraf --glob 'ssr-dist'",
    "build": "yarn run prebuild:ssr && yarn run build:ssr && yarn run prebuild:client && yarn run build:client",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "prettier --write .",
    "test": "jest ./"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.3",
    "@testing-library/jest-dom": "^5.16.5",
    "antd": "^5.3.2",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.2",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.9.0",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.8",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitejs/plugin-react": "^2.0.1",
    "eslint": "^8.23.0",
    "history": "^5.3.0",
    "jest": "^28",
    "jest-css-modules-transform": "^4.4.2",
    "jest-environment-jsdom": "^29.0.1",
    "lefthook": "^1.1.1",
    "prettier": "^2.7.1",
    "rimraf": "5.0.1",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2",
    "vite": "3.1.0",
    "vite-plugin-pwa": "^0.14.7",
    "whatwg-fetch": "^3.6.2",
    "workbox-precaching": "^6.5.4"
  },
  "license": "MIT"
}
